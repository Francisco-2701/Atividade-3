# Lista inicial
animais = ['cachorro', 'gato', 'papagaio']

animais.append('hamster')


animais.insert(1, 'coelho')

animais.pop()

animais.pop(2)

animais[0] = 'le√£o'

print(animais)

# segunda lista

matriz = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
numero_cinco = matriz[1][1]
print(numero_cinco)

# terceira lista 

exemplo = [10, 20, [30, 40], "hello"]
comprimento = len(exemplo)
print(comprimento)

# quarta lista 

frutas = ['morango', 'abacaxi', 'goiaba', 'jambo', 'banana']

# Comando para retornar o primeiro elemento da lista:
primeiro_elemento = frutas[0]
print(primeiro_elemento)

# Comando para retornar o terceiro elemento da lista:
terceiro_elemento = frutas[2]
print(terceiro_elemento)
